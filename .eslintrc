{
	"env": {
		"browser": true,
			"es6": true,
			"node": true,
	},
		"extends": "airbnb",
		"parser": "babel-eslint",
		"parserOptions": {
			"ecmaFeatures": {
				"experimentalObjectRestSpread": true,
			},
			"sourceType": "module",
		},
		"rules": {
			"no-console": [
				"error",
			{
				"allow": [
					"warn",
				"error"
				]
			}
			],
			"no-multi-assign": 0,
			"indent": [
				"error",
			"tab",
			],
			"linebreak-style": [
				"error",
			"unix",
			],
			"no-tabs": 0,
			"quotes": [
				"error",
			"double",
			],
			"semi": [
				"error",
			"always",
			],
		},
}
